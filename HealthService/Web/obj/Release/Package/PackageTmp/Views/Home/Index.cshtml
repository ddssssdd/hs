@{
    ViewBag.Title = "Home Page";
}
@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>
            
        </div>
    </section>
}
@foreach (var item in ViewBag.tables)
{
   
    <p>@item @Html.ActionLink("Open", "TableContent", new { tablename = item })</p>
}



